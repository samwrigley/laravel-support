{
    "name": "samwrigley/laravel-support",
    "description": "A collection of helpers for quickly and easily building Laravel applications",
    "homepage": "https://github.com/samwrigley/laravel-support",
    "authors": [
        {
            "name": "Sam Wrigley",
            "email": "sam@samwrigley.co.uk",
            "homepage": "https://samwrigley.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^5.4|^5.5|^5.6|^5.7|^5.8|^6.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.1",
        "orchestra/testbench": "^3.6"
        "phpunit/phpunit": "^8.0",
    },
    "autoload": {
        "psr-4": {
            "SamWrigley\\Support\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SamWrigley\\Support\\Test\\": "tests"
        }
    }
}
